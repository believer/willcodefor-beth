{
  "name": "willcodefor",
  "version": "0.0.0",
  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "dev": "concurrently \"bun run --watch src/index.tsx\" \"bun run tw:dev\"",
